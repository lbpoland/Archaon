main_docs (["main":({"This is the include file for the group system.  It is includedby at least the handler, the effect and the commands.         ",}),"author":({"Tape",}),])
define_docs (["EFFECT":(["main":({"This points to the location of the group effect.",}),]),"PRIMARY_BONUS_MULTIPLIER":(["main":({"A member's personal rating:<br>  ( collective primary bonuses / number of bonuses *   PRIMARY_BONUS_MULTIPLIER ) + ( bonus in fighting *   FIGHTING_BONUS_MULTIPLIER )<p>A member's xp share: <br>   Total xp from a kill *   a member's personal rating / group's combined   ratings",}),]),"MAX_INVITATIONS_PER_PARTY_AT_ONCE":(["main":({"The maximum number of invitations that can at one time bepending.",}),]),"INVITE_TIMEOUT":(["main":({"Invitations automatically time out after this many seconds.",}),]),"GROUP_SUBCOMMAND_INHERITABLE":(["main":({"This points to the group sub-command inheritable.",}),]),"GROUP_NOTIFY_PREFIX":(["main":({"This is sticked to the beginning of all group messages.  $channel$is replaced by the group's channel name after making it look pretty.<p>The $colour$ tag is replaced with the custom group colour, if anyhas been set.<p>Note: the $I$ bit must be there somewhere, unless you feel likerewriting the group history function.  Plus it looks nice, sowhat the hell.  Mind, it should be in the form \"$I$+x,+y=\"",}),]),"GROUP_COLOUR_EVENT":(["main":({"This defines what the custom colour label for the group iscalled.",}),]),"CONTEMPLATE":(["main":({"Path to the contemplate effect/command.",}),]),"GROUP_CMD":(["main":({"This points to the location of the master group command file.",}),]),"GROUP_TITLE":(["main":({"Name of the title type for a group used by set_title().",}),]),"GROUP":(["main":({"This points to the location of the group handler.",}),]),"FIGHTING_BONUS_MULTIPLIER":([]),"MAX_GROUP_NAME_SIZE":(["main":({"This defines the maximum number of characters you can havein a group name.",}),]),"MAX_PEOPLE_PER_PARTY":(["main":({"The maximum number of members a party can contain at a time.",}),]),"MAX_PEOPLE_YOU_CAN_ASSIST":(["main":({"The maximum number of people you can be assisting at a time.",}),]),"INVALID_WORDS":(["main":({"These words are automatically filtered out when convertinga group's short to its name with short_to_name().  They'renot case sensitive.",}),]),"MIN_EXPERIENCE":(["main":({"If shared xp gained is below this number, it isn't shared.",}),]),"GROUP_SUB_CMDS_DIR":(["main":({"This defines where the master group command looks forsub commands.",}),]),"GROUP_SUB_CMDS_FILE_WILDCARD":(["main":({"This is the wildcard used to find the commands in the directory.",}),]),"SHADOW_OB":(["main":({"This points to the location of the group shadow.",}),]),"USE_BASE_STATS":(["main":({"Set this to 1 if you want base stats (the ones the playerrearranged to) to be used for calculating xp share, insteadof the player's current stats.",}),]),"GROUP_CHANNEL_MAX_CHARS":(["main":({"This defines how many characters of the group channel name is displayeduntil it's cut off with three periods (...)  E.g. [groupna...] Tape: Bing.It must be larger than 3, or 0 if no periods should be added.",}),]),])
