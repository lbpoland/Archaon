define_docs (["AUCTION_EVENT_INFO":(["main":({"This event is when the player does an 'info' command to find outhow long each of the auctions is.  The extra argument is the generatedinfo string.",}),]),"AUCTION_EVENT_LIST":(["main":({"The event for listing the contents of the shop.  The extra arguement isthe generated shoip list text.",}),]),"AUCTION_FINISH_PHASE":(["main":({"This define is used in the function for the item for thefinish auction phase.",}),]),"AUCTION_EVENT_CANNOT_WITHDRAW":(["main":({"The event when an item is unable to be withdrawn.  The extra arguements arethe auction class of the withdrawn item.",}),]),"AUCTION_EVENT_ADD_AUCTION":(["main":({"The event when the player is about to try and add something into the shop.The extra arguments are the name to sell it as, the objects to sell,the reserve code and the length of the auction and the generateddefault string.",}),]),"AUCTION_EVENT_WITHDRAW":(["main":({"The event when an item is withdrawn.  The extra arguements arethe auction class of the withdrawn item.",}),]),"AUCTION_EVENT_BAD_MONEY":(["main":({"The event when the player is giving a bad money spec.   Theextra arguments are the name to sell it as, the objects to sell,the reserve code and the length of the auction.",}),]),"AUCTION_CLAIM_PHASE":(["main":({"This define is used in the function for the item for theclaim the item auction phase.",}),]),"AUCTION_WITHDRAW_PHASE":(["main":({"This define is used in the function for the item for thewithdraw auction phase.",}),]),"AUCTION_EVENT_TOO_POOR":(["main":({"The event when the bidder is too poor.  The extra arguements areauction class, the players money, the value of the placed bid.",}),]),"AUCTION_EVENT_BID_TOO_SMALL":(["main":({"The event when the bid is too small.  The extra arguements are theauction class, the value of the placed bid, the new minimum bid.",}),]),"AUCTION_EVENT_CONFIRM_AUCTION":(["main":({"Ths event when the player has completely added the auction to the shop.The extra arguments are the name to sell it as, the objects to sell,the reserve code and the length of the auction and the generateddefault string.",}),]),"AUCTION_EVENT_FULL":(["main":({"The event when the shop is too full to handle the extra item.  Theextra arguments are the name to sell it as, the objects to sell,the reserve code and the length of the auction.",}),]),"AUCTION_EVENT_ADD_ABORT":(["main":({"Ths event when the player has aborted adding an auction to the shop.The extra arguments are the name to sell it as, the objects to sell,the reserve code and the length of the auction.",}),]),"AUCTION_EVENT_CLAIM":(["main":({"The event when an item is to be claimed.  The extra arguements arethe auction class of the claimed item, the bid and the objects claimed.",}),]),"AUCTION_EVENT_BROWSE":(["main":({"The event for browsing a specific item for auction.  The extra argumentsare the auction class, the objects and the generated string.",}),]),"AUCTION_EVENT_BID_BELOW_RESERVE":(["main":({"The event when the bid is below the reserve.  The extra arguements are theauction class, the value of the placed bid, the reserve.",}),]),"AUCTION_EVENT_BID":(["main":({"The even when a bid is made.  The extra arguements are the auctionclass and the bid made.",}),]),"AUCTION_EVENT_NOTHING_TO_CLAIM":(["main":({"The event when the person has nothing to claim.  No extra arguements.",}),]),"AUCTION_EVENT_BAD_ITEM":(["main":({"The item index was bad, unable to find the item. The extra argument is theid that was bad.",}),]),"AUCTION_RETRY_PHASE":(["main":({"This define is used in the function for the item for theclaim the item auction phase.",}),]),])
